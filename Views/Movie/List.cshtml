@model IEnumerable<DemoMvc.Models.Movie>
<h1 align="center">List Page in MovieController</h1>
<h2>List of Movies</h2>
<hr />
<table>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Id)</th>
            <th>@Html.DisplayNameFor(m => m.Name)</th>
            <th>@Html.DisplayNameFor(m => m.Year)</th>
            <th>@Html.DisplayNameFor(m => m.Rating)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        { 
        <tr>
            <td>@Html.DisplayFor(m => data.Id)</td>
            <td>@Html.DisplayFor(m => data.Name)</td>
            <td>@Html.DisplayFor(m => data.Year)</td>
            <td>@Html.DisplayFor(m => data.Rating)</td>
        </tr>
        }
    </tbody>
</table>